<!DOCTYPE html>
<html>
	<head>
		<title>Project 1</title>
	</head>
	<body>
		<h1> Robert "Bryce" Simmer </h1>
		<table>
			<thead>
				<th>Project Name</th>
				<th>Description</th>
				<th>URL</th>
			</thead>
			<tbody>
				<% PROJECTS.each do |project, details| %>
					<tr>
						<td><%= project %></td>
						<td><%= details['description'] %></td>
						<td><%= link_to details['url'], details['url'] %></td>
					</tr>
				<% end %>				
			</tbody>
		</table>
	</body>
</html>